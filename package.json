{
  "name": "code-review-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "functions-framework --target=handler --source=dist/index.js",
    "local": "ts-node --loader tsconfig-paths/esm --experimental-specifier-resolution=node src/index.ts",
    "dev": "nodemon --watch src --exec \"pnpm run local\""
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.18",
    "@google-cloud/functions-framework": "^3.5.1",
    "@octokit/rest": "^21.1.1",
    "@octokit/webhooks": "^13.8.0",
    "ai": "^4.3.9",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "parse-diff": "^0.11.1",
    "zod": "^3.24.3"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/node": "^20",
    "eslint": "^9",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "tsconfig-paths": "^4.2.0",
    "nodemon": "^3.0.1"
  }
}
